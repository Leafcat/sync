#users-header
  margin-top 30px
  width 100%
  height 32px
  line-height 32px
  display flex
  text-align center
  
#users-header .moderator
  display inline-block
  min-width 40px

#users-header .username
  display inline-block
  width 100%
  border-left 1px solid

#users-header .time
  display inline-block
  width 60px
  flex-shrink 0
  border-left 1px solid

#users li
  list-style-type none

#users .user .flex
  display flex
  width 100%
  background-color #FFFFFF
  height 32px
  margin-top 5px
  color #424242
  line-height 30px

#users .user .flex .moderator
  width 40px
  flex-shrink 0
  line-height 32px
  text-align center
  cursor default
  &:hover span.prev-star:before
    color #ffc107
    content "\f005"
    cursor pointer

#users .user .flex .username
  height 100%
  width 100%
  border-left 1px solid #424242
  padding-left 5px
  text-align center
  vertical-align middle
  overflow hidden
  white-space nowrap
  text-overflow ellipsis

#users .user .flex .mod-buttons
  flex-shrink 0
  padding-left 5px
  padding-right 5px
  border-left 1px solid #424242
  
#users .user .flex .mod-buttons>span
  width 32px
  height 100%
  display inline-block
  text-align center
  cursor pointer

#users .user .flex .time
  border-left 1px solid #424242
  height 100%
  width 60px
  flex-shrink 0
  line-height 32px
  padding-left 8px

#users .user .progress
  position relative
  height 2px
  width 100%
  top -2px

#users .user .progress .bufferProgress
  height 100%
  position absolute
  top 0 
  left 0
  background-color #b2b2b2
  transition width 0.5s linear
  
#users .user .progress .timeProgress
  height 100%
  position absolute
  top 0 
  left 0
  background-color #8942d7
  transition width 0.5s linear
